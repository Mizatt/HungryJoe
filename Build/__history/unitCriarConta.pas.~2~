unit unitCriarConta;

interface

uses
  Winapi.Windows, Winapi.Messages, System.SysUtils, System.Variants, System.Classes, Vcl.Graphics,
  Vcl.Controls, Vcl.Forms, Vcl.Dialogs, Vcl.Imaging.pngimage, Vcl.ExtCtrls;

type
  TformCriarConta = class(TForm)
    Image1: TImage;
    Image2: TImage;
    procedure Image2Click(Sender: TObject);
  private
    { Private declarations }
  public
    { Public declarations }
  end;

var
  formCriarConta: TformCriarConta;

implementation

{$R *.dfm}

uses unitLogin;

procedure TformCriarConta.Image2Click(Sender: TObject);
begin
    AnimateWindow(formLogin.Handle,500,AW_HOR_NEGATIVE);
    Self.Close;
end;

end.
